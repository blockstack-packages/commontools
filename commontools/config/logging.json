{
	"version": 1,
	"disable_existing_loggers": false,
	"formatters": {
		"simple": {
			"format": "(%(levelname)s:\t%(asctime)s) %(name)s:\t%(message)s"
		}
	},

	"handlers": {
		"console": {
			"class": "logging.StreamHandler",
			"level": "DEBUG",
			"formatter": "simple",
			"stream": "ext://sys.stdout"
		}

	},

	"loggers": {
		"my_module": {
			"level": "ERROR",
			"handlers": ["console"],
			"propagate": "no"
		}
	},

	"root": {
		"level": "DEBUG",
		"handlers": ["console"]
	}
}